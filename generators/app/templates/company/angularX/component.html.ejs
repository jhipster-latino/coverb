<section id="<%=_.kebabCase(COVER_NAME)%>">
  <div class="jumbotron text-center">
    <span id="logo"></span>
    <%-ENABLE_TRANSLATION ? `<h1 jhiTranslate="${_.kebabCase(COVER_NAME)}.name">`:`<h1>`%>Jhipster Latino</h1>
    <%-ENABLE_TRANSLATION ? `<p jhiTranslate="${_.kebabCase(COVER_NAME)}.description">`:`<p>`%>Development tools and courses for build better jhipster applications</p>
  </div>
  <div class="container-fluid text-center">
    <%-ENABLE_TRANSLATION ? `<h2 jhiTranslate="${_.kebabCase(COVER_NAME)}.service.title">`:`<h2>`%>Services</h2>
    <%-ENABLE_TRANSLATION ? `<p jhiTranslate="${_.kebabCase(COVER_NAME)}.service.description">`:`<p>`%>What we offer?</p>
    <br>
    <div class="row">
      <div class="col-sm-4">
        <fa-icon [icon]="'bullhorn'"></fa-icon>
        <h4>Training</h4>
        <p>Improvement your carrear</p>
      </div>
      <div class="col-sm-4">
        <fa-icon [icon]="'chart-line'"></fa-icon>
        <h4>Tools</h4>
        <p>Optimice your time</p>
      </div>
      <div class="col-sm-4">
        <fa-icon [icon]="'magic'"></fa-icon>
        <h4>Design</h4>
        <p>Details are important</p>
      </div>
    </div>
  </div>
  </div>
  <div class="container-fluid text-center bg-gray">
    <%-ENABLE_TRANSLATION ? `<h2 jhiTranslate="${_.kebabCase(COVER_NAME)}.portafolio.title">`:`<h2>`%>Portafolio</h2>
    <%-ENABLE_TRANSLATION ? `<p jhiTranslate="${_.kebabCase(COVER_NAME)}.portafolio.description">`:`<p>`%>What did we already created?</p>
    <br>
    <div class="row text-center">
      <div class="col-sm-4">
        <div class="thumbnail">
          <fa-icon [icon]="'cubes'"></fa-icon>
          <h4>coverb</h4>
          <p>Customice you client</p>
        </div>
      </div>
      <div class="col-sm-4">
        <div class="thumbnail">
          <fa-icon [icon]="'cogs'"></fa-icon>
          <h4>moduleb</h4>
          <p>Build your own module</p>
        </div>
      </div>
      <div class="col-sm-4">
        <div class="thumbnail">
          <fa-icon [icon]="'server'"></fa-icon>
          <h4>engineb</h4>
          <p>Data managment</p>
        </div>
      </div>
    </div>
  </div>
  <div class="container-fluid">
    <div class="text-center">
      <%-ENABLE_TRANSLATION ? `<h2 jhiTranslate="${_.kebabCase(COVER_NAME)}.pricing.title">`:`<h2>`%>Pricing</h2>
      <%-ENABLE_TRANSLATION ? `<p jhiTranslate="${_.kebabCase(COVER_NAME)}.pricing.description">`:`<p>`%>Choose a payment plan that works for you</p>
      <br>
    </div>
    <div class="row">
      <div class="col-sm-4">
        <div class="panel panel-default text-center">
          <div class="panel-heading">
            <h1>Basic</h1>
          </div>
          <div class="panel-body">
            <br>
            <p>Access to a lot of training content</p>
            <p>Access to source our product</p>
            <p>Comunity support</p>
          </div>
          <div class="panel-footer">
            <hr>
            <h3>Free</h3>
            <h4>Just say thank you</h4>
            <button class="btn btn-lg">Sign Up</button>
          </div>
        </div>
      </div>
      <div class="col-sm-4">
        <div class="panel panel-default text-center">
          <div class="panel-heading">
            <h1>Pro</h1>
          </div>
          <div class="panel-body">
            <br>
            <p>Full access to training content</p>
            <p>Full access to our premiun products</p>
            <p>Access to source our product</p>
          </div>
          <div class="panel-footer">
            <hr>
            <h3>$10</h3>
            <h4>per month</h4>
            <button class="btn btn-lg">Sign Up</button>
          </div>
        </div>
      </div>
      <div class="col-sm-4">
        <div class="panel panel-default text-center">
          <div class="panel-heading">
            <h1>Premium</h1>
          </div>
          <div class="panel-body">
            <br>
            <p>You can request features for products</p>
            <p>Full support from our team</p>
            <p>All our <strong>Pro plan</strong> features</p>
          </div>
          <div class="panel-footer">
            <hr>
            <h3>$25</h3>
            <h4>per month</h4>
            <button class="btn btn-lg">Sign Up</button>
          </div>
        </div>
      </div>
    </div>
  </div>
